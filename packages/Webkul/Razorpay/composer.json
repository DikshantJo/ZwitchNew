{
    "name": "webkul/razorpay",
    "description": "Razorpay payment integration for Bagisto",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webkul",
            "email": "support@webkul.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\Razorpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webkul\\Razorpay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\Razorpay\\Providers\\RazorpayServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 